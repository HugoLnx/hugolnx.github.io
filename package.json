    "bulmaswatch": "^0.8.1",
    "compression": "^1.7.4",
    "cross-fetch": "^4.0.0",
    "js-logger": "^1.6.1",
    "material-icons": "^1.13.12",
    "three": "^0.161.0",
    "tweakpane": "^4.0.3",
    "unplugin-vue-markdown": "^0.26.0",
    "vike": "^0.4.160",
    "vike-vue": "^0.6.1",
    "vue": "^3.4.15",
    "vue-gtag": "^2.0.1"
